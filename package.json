{
  "name": "devs-social-app-server",
  "version": "1.0.0",
  "description": "Server for devs social app",
  "main": "server.js",
  "author": "Jorge Mayoral Alvarez",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=production node src/server.js",
    "dev": "NODE_ENV=development nodemon src/server.js",
    "test": "NODE_ENV=test PORT=5005 jest --verbose --silent",
    "test:token": "NODE_ENV=test PORT=5005 jest --verbose --silent \"token\"",
    "test:user": "NODE_ENV=test PORT=5005 jest --verbose --silent \"user\"",
    "test:post": "NODE_ENV=test PORT=5005 jest --verbose --silent \"post\"",
    "test:watch": "npm test --watch",
    "mock:users": "node src/mock/mockUsers.js",
    "mock:posts": "node src/mock/mockPosts.js"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "cors": "2.8.5",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "express-async-handler": "1.1.4",
    "helmet": "4.6.0",
    "jsonwebtoken": "8.5.1",
    "mongoose": "5.13.0",
    "morgan": "1.10.0"
  },
  "devDependencies": {
    "jest": "27.0.6",
    "nodemon": "2.0.8",
    "supertest": "6.1.3"
  }
}
